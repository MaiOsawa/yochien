<h1>スケジュール更新</h1>

<%= form_with model: @schedule, url: schedule_path, method: "patch", local: true do |f| %>

    <div class="form-group">
        <label>年</label>
        <%= f.text_field :year, class: "form-control" %>
    </div>
    
    <div class="form-group">
        <label>月</label>
        <%= f.text_field :month, class: "form-control" %>
    </div>
    
    <div class="form-group">
        <label>日</label>
        <%= f.text_field :day, class: "form-control" %>
    </div>
    
    <div class="form-group">
        <label>場所</label>
        <%= f.text_field :place, class: "form-control" %>
    </div>
    
    <%= f.submit "更新", class: "btn btn-primary" %>

<% end %>